{
  "name": "product",
  "version": "0.0.1",
  "private": true,
  "description": "product project.",
  "scripts": {
    "start": "better-npm-run start:dev",
    "start:dev": "better-npm-run start:dev",
    "start:prod": "better-npm-run start:prod",
    "clean": "better-npm-run clean:dist",
    "clean:dist": "better-npm-run clean:dist",
    "clean:node_modules": "better-npm-run clean:node_modules",
    "reinstall": "better-npm-run reinstall",
    "compile": "better-npm-run compile:prod",
    "compile:dev": "better-npm-run compile:dev",
    "compile:prod": "better-npm-run compile:prod",
    "compile:analyze": "better-npm-run compile:analyze:prod",
    "compile:analyze:dev": "better-npm-run compile:analyze:dev",
    "compile:analyze:prod": "better-npm-run compile:analyze:prod",
    "test": "better-npm-run test",
    "lint": "eslint src bin config",
    "lint:fix": "eslint src bin config --fix"
  },
  "betterScripts": {
    "start:dev": {
      "command": "node bin/server.js",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "node bin/server.js",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "clean:dist": {
      "command": "rimraf dist"
    },
    "clean:node_modules": {
      "command": "rimraf node_modules"
    },
    "reinstall": {
      "command": "yarn run clean:node_modules && yarn install"
    },
    "compile:dev": {
      "command": "yarn run clean:dist && node bin/compile",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "compile:prod": {
      "command": "yarn run clean:dist && node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile:analyze:dev": {
      "command": "yarn run clean:dist && node bin/compile",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*",
        "ANALYZE": true
      }
    },
    "compile:analyze:prod": {
      "command": "yarn run clean:dist && node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*",
        "ANALYZE": true
      }
    },
    "test": {
      "command": "node node_modules/.bin/jest --no-cache --coverage",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "debug": "^2.6.6",
    "idx": "^1.5.0",
    "immutable": "4.0.0-rc.2",
    "ip": "^1.1.5",
    "lodash": "^4.15.0",
    "moment": "^2.18.1",
    "object-assign": "^4.1.1",
    "prop-types": "^15.5.8",
    "react": "15.4.2",
    "react-bootstrap": "^0.31.0",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dom": "15.4.2",
    "react-dropzone": "^3.13.1",
    "react-file-download": "^0.3.4",
    "react-helmet": "^5.0.3",
    "react-lazyload": "^2.2.7",
    "react-list": "^0.8.5",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "react-select": "^1.0.0-rc.3",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.1.0",
    "reselect": "^3.0.0",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-lodash": "^3.2.8",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "better-npm-run": "^0.0.15",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fs-extra": "^3.0.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "nock": "^9.0.13",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "15.4.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  }
}
